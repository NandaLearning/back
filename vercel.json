{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      { "src": "/api/data", "dest": "index.js" },
      { "src": "/api/data/:id", "dest": "index.js" }
    ],
    "env": {
        "DATABASE_URL": "@DATABASE_URL"
    }
  }
  